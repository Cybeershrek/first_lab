CC = gcc
CFLAGS = -Wall -Wextra -std=c11
TARGET = lab

SRCS = main.c integer.c char.c my_string.h
OBJS = $(SRCS:.c=.o)
DEPS = char.h integer.h  my_string.h stringErros.h TypeInfo.h

all: $(TARGET)

$(TARGET): $(OBJS)
	$(CC) $(CFLAGS) $(OBJS) -o $(TARGET)

%.o: %.c $(DEPS)
	$(CC) $(CFLAGS) -c $< -o $@

clean:
	rm -f $(OBJS) $(TARGET)